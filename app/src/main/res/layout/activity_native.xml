<?xml version="1.0" encoding="utf-8"?>
  <LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cbnativetest.NativeActivity">

  <TextView
      android:id="@+id/txtUesrInfo"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Hello World!"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintLeft_toLeftOf="parent"
      app:layout_constraintRight_toRightOf="parent"
      app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/test1"
        android:text="call test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="callSBAPI"
        >

    </Button>

    <Button
        android:id="@+id/test2"
        android:text="sso test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="getSSO"
        >

    </Button>

    <Button
        android:id="@+id/test3"
        android:text="doc test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="DocView"
        >

    </Button>

  </LinearLayout>